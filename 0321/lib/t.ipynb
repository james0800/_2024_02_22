   "source": [
    "import random\n",
    "def f(num:int):\n",
    "    with open(\"names.txt\",encoding=\"utf8\") as file:\n",
    "        con=file.read()\n",
    "        arr:list[str]=con.split('\\n')\n",
    "        name=random.choices(arr,k=num)\n",
    "        return name\n",
    "        file.close()\n",
    "names:list[str]=f(5)\n",
    "names\n"
   ]
  }
